<?xml version="1.0" encoding="UTF-8"?>
<!--

  Simple XML that represent a computer.
  The tag keyboard must contain a string.
  Mouse has to match a patter "x button".
  Monitor must contain a valid standard name.

-->
<listaComputer
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="schemaComputer.xsd">
	<computer>
                <codice>1</codice>
		<keyboard>105 keys</keyboard>
		<mouse>3 button</mouse>
		<monitor>vga</monitor>
	</computer>
	<computer>
                <codice>2</codice>
		<keyboard>115 keys</keyboard>
		<mouse>2 button</mouse>
		<monitor>svga</monitor>
	</computer>
	<computer>
                <codice>3</codice>
		<keyboard>115 keys</keyboard>
		<mouse>3 button</mouse>
		<monitor>svga</monitor>
	</computer>
</listaComputer>